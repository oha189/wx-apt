.seek {
  height: 100rpx;
  background: #FC304E;
  display: flex;
  justify-content: center;
  align-items: center;
}
.seek input {
  width: 90%;
  height: 80%;
  background: #fff;
  color: #7C7C7C;
  text-align: center;
  border-radius: 6rpx;
}
.slideshow {
  height: 350rpx;
}
.slideshow swiper-item image {
  width: 100%;
  height: 80%;
}
.guide image {
  width: 150rpx;
  height: 150rpx;
}
#clothing {
  margin: 0 10rpx;
}
#clothing .headline {
  color: #FFCBD4;
  margin: 25rpx 0;
}
#clothing .clothing-img {
  height: 380rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
#clothing .clothing-img .imgs {
  width: 32%;
  height: 170rpx;
}
